-f ./tags
--langmap=php:+.inc
-h '.php.inc'
-R
--totals=yes
--tag-relative=yes
--PHP-kinds=+cf-v
--regex-PHP
--regex-PHP=/(^|^[^\/*]+[[:blank:]])define[[:blank:]]*\([[:blank:]]*['\"]?([[:alpha:]_][[:alnum:]_]*)/\2/d/
--regex-PHP=/(^|^[^\/*]+[[:blank:]])class[[:blank:]]+([[:alpha:]_][[:alnum:]_]*)/\2/c/
--regex-PHP=/(^|^[^\/*]+[[:blank:]])function[[:blank:]]+&?[[:blank:]]*([[:alpha:]_][[:alnum:]_]*)/\2/f/
